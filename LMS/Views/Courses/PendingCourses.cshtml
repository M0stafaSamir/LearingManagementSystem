@model IEnumerable<LMS.Models.InstractourModel.Course>

@{
    ViewData["Title"] = "PendingCourses";
}

<section id="courses" class="courses section">

    <div class="container">
<h1>PendingCourses</h1>

        <div class="row">


<div class="row g-3">

    @foreach (var item in Model)
    {
        <div class="col-12 col-md-6 col-lg-3">

            <partial name="_CourseCard" for="@item"></partial>
        </div>
          
        
    }

</div>
</div>
</div>
</section>
