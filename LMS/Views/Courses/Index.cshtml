@model IEnumerable<LMS.Models.InstractourModel.Course>

@{
    ViewData["Title"] = "Index";
}
@* <h1>Index</h1> *@

<div class="my-5">
<p>
    <a asp-action="Create" class="fw-bold">+Add New Course</a>
</p>
<div class="row ">

    @foreach (var item in Model)
    {
            <div class="col-12 col-md-6 col-lg-3">

                <partial name="_CourseCard" for="@item"></partial>
            </div>

        }

</div>
</div>